algoritmo "semnome"

var
nome: caractere
salario: inteiro
acrescimo: real
inicio
ESCREVAL ("Qual o seu nome?")
LEIA (nome)

ESCREVAL ("Qual o seu salario ",nome, "?")
LEIA (salario)

acrescimo:=salario * 1.10
ESCREVAL ("Seu salario com acrescimo de 10% ficara ", acrescimo)

fimalgoritmo
