algoritmo "Exercicio_5"

var
num1: real
num2: real
total: real

inicio


repita
   escreval("Digite um valor: ")
   leia(num1)

   escreval("Digite outro valor: ")
   leia(num2)

ate ((num1 >= 1) e (num1 <=100)) ou ((num2 >=1) e (num2 <=100))

total:= num1 + num2
Escreval("Total: ",total)

fimalgoritmo
